{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "singularName": "movie",
    "pluralName": "movies",
    "displayName": "Movie",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "time": {
      "type": "integer",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "movies"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::movie.movie",
      "inversedBy": "movies"
    },
    "movies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::movie.movie",
      "mappedBy": "author"
    }
  }
}
